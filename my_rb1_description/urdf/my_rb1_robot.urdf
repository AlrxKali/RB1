    </inertial> <visual> <geometry> <sphere radius="0.025"/> </geometry> <origin xyz="0 0 0"/> <material name="grey"/> </visual> <collision> 
      <geometry>
        <sphere radius="0.025"/> </geometry> <origin xyz="0 0 0"/> </collision> </link> <joint name="front_caster_joint" type="fixed"> <parent 
    link="base_link"/> <child link="front_caster"/> <origin xyz="0.1 0.0 -0.15" rpy="0 0 0"/>
  </joint> <!-- Back Caster --> <link name="back_caster"> <inertial> <origin xyz="0 0 0"/> <mass value="0.5"/> <inertia ixx="0.000125" 
